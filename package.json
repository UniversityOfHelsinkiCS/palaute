{
  "name": "palaute",
  "version": "1.0.0",
  "description": "Norppa is THE software for student feedback",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up",
    "bash": "docker-compose run app bash",
    "reset": "docker-compose down -v && docker-compose up --build",
    "build": "react-app-rewired build",
    "start:front": "react-app-rewired start",
    "start:prod": "NODE_ENV=production node ./src/server/index.js",
    "start:test": "NODE_ENV=test node ./src/server/index.js",
    "start:dev": "NODE_ENV=development concurrently \"nodemon --inspect=0.0.0.0 -L ./src/server/index.js\" \"CHOKIDAR_USEPOLLING=true react-app-rewired start\" ",
    "test": "npm run test:setup && npm run test:e2e",
    "test:setup": "docker-compose down && docker-compose -f docker-compose.ci.yml rm -vf && docker-compose -f docker-compose.ci.yml up -d --build",
    "test:e2e": "cypress run",
    "test:cypress": "cypress open",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "prepare": "husky install",
    "tira2": "docker exec -it palaute node src/server/util/tira2util.js"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "*.{js,json,css,md}": "prettier --write"
  },
  "engines": {
    "node": ">=14.7"
  },
  "author": "Jami Kousa",
  "license": "ISC",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@material-ui/pickers": "^3.3.10",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.89",
    "@mui/material": "^5.8.7",
    "@mui/styles": "^5.8.7",
    "@sentry/browser": "^6.7.2",
    "@sentry/node": "^6.7.2",
    "@sentry/tracing": "^6.7.2",
    "axios": "^0.21.4",
    "babel-eslint": "^10.1.0",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "chart.js": "^3.4.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "date-fns": "^2.19.0",
    "date-fns-timezone": "^0.1.4",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "formik": "^2.2.6",
    "highcharts": "^9.1.2",
    "highcharts-react-official": "^3.0.0",
    "husky": "^7.0.0",
    "i18next": "^20.3.2",
    "immer": "^9.0.15",
    "intersection-observer": "^0.12.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.19",
    "morgan": "^1.10.0",
    "notistack": "^1.0.6",
    "papaparse": "^5.3.0",
    "path-browserify": "^1.0.1",
    "pg": "^8.5.1",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "qs": "^6.7.0",
    "react": "^17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-chartjs-2": "^3.0.2",
    "react-csv": "^2.0.3",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.12",
