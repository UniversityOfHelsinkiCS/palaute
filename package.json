    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "minimist": "^1.2.7",
    "nodemon": "^3.1.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "react-error-overlay": "^6.0.11",
    "rimraf": "^5.0.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "util": "^0.12.4",
    "vite-plugin-eslint": "^1.8.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "cypress",
      "src/client",
      "build/client",
      "**/**.log"
    ],
    "delay": "2500"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  }
}
