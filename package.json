{
  "name": "palaute",
  "version": "1.0.0",
  "description": "Norppa is THE software for student feedback",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up",
    "bash": "docker-compose run app bash",
    "reset": "docker-compose down -v && docker-compose up --build",
    "build": "react-scripts build",
    "start:front": "react-scripts start",
    "start:prod": "NODE_ENV=production node ./src/server/index.js",
    "start:test": "NODE_ENV=test node ./src/server/index.js",
    "start:dev": "NODE_ENV=development concurrently \"nodemon --inspect=0.0.0.0 -L ./src/server/index.js\" \"CHOKIDAR_USEPOLLING=true react-scripts start\" ",
    "test": "jest --verbose --runInBand --silent=false --forceExit",
    "test:e2e": "cypress run",
    "test:cypress": "cypress open",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "prepare": "husky install",
    "tira2": "docker exec -it palaute node src/server/util/tira2util.js"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "*.{js,json,css,md}": "prettier --write"
  },
  "engines": {
    "node": ">=14.7"
  },
  "author": "Jami Kousa",
  "license": "ISC",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@sentry/browser": "^6.7.2",
    "@sentry/node": "^6.7.2",
    "@sentry/tracing": "^6.7.2",
    "axios": "^0.21.4",
    "chart.js": "^3.4.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "date-fns": "^2.19.0",
    "date-fns-timezone": "^0.1.4",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "formik": "^2.2.6",
    "highcharts": "^9.1.2",
    "highcharts-react-official": "^3.0.0",
    "husky": "^7.0.0",
    "i18next": "^20.3.2",
    "immer": "^9.0.15",
    "intersection-observer": "^0.12.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.19",
    "morgan": "^1.10.0",
    "notistack": "^1.0.6",
    "papaparse": "^5.3.0",
    "pg": "^8.5.1",
    "prettier": "^2.3.2",
    "qs": "^6.7.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.2",
    "react-csv": "^2.0.3",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.12",
    "react-intersection-observer": "^8.31.1",
    "react-markdown": "^6.0.2",
    "react-qr-code": "^2.0.3",
    "react-query": "^3.18.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redis": "^4.0.4",
    "resize-observer-polyfill": "^1.5.1",
    "sequelize": "^6.6.4",
    "supertest": "^6.1.3",
    "umzug": "^2.3.0",
    "unfuck-spa-shibboleth-session": "^3.1.3",
    "unfuck-utf8-headers-middleware": "^1.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.0.0",
    "cypress": "^7.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "nodemon": "^2.0.7",
    "react-error-overlay": "6.0.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "palaute/*",
      "client/*"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  },
  "proxy": "http://localhost:8000"
}
