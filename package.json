    "immer": "^9.0.19",
    "intersection-observer": "^0.12.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "lru-cache": "^7.14.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "notistack": "^2.0.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pg": "^8.5.1",
    "process": "^0.11.10",
    "qs": "^6.7.0",
    "react": "^18.2.0",
