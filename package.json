{
  "name": "tuni-kpj",
  "version": "1.0.0",
  "description": "Norppa is THE software for student feedback",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up",
    "bash": "docker-compose run app bash",
    "reset": "docker-compose down -v && docker-compose up --build",
    "build": "DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "start:front": "react-app-rewired start",
    "start:prod": "NODE_ENV=production node ./src/server/index.js",
    "start:test": "NODE_ENV=test node ./src/server/index.js",
    "start:dev": "NODE_ENV=development concurrently \"nodemon --inspect=0.0.0.0 -L ./src/server/index.js\" \"CHOKIDAR_USEPOLLING=true react-app-rewired start\" ",
    "test": "npm run test:setup && npm run test:e2e",
    "test:setup": "docker-compose down && docker-compose -f docker-compose.ci.yml rm -vf && docker-compose -f docker-compose.ci.yml up -d --build",
    "test:e2e": "cypress run",
    "test:cypress": "cypress open",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "prepare": "husky install",
    "translations": "node ./tools/analyzeTranslations.js"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "*.{js,json,css,md}": "prettier --write"
  },
  "engines": {
    "node": ">=14.7"
  },
  "author": "Jami Kousa",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.114",
    "@mui/material": "^5.11.3",
    "@mui/x-date-pickers": "^5.0.0-beta.5",
    "@sentry/browser": "^6.7.2",
    "@sentry/node": "^6.7.2",
    "@sentry/tracing": "^6.7.2",
    "axios": "^0.21.4",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "chart.js": "^3.4.0",
    "chartjs-adapter-date-fns": "^2.0.0",
    "chartjs-plugin-annotation": "^2.0.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "date-fns": "^2.19.0",
    "date-fns-timezone": "^0.1.4",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "formik": "^2.2.6",
    "i18next": "^20.3.2",
