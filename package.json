    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@sentry/browser": "^6.2.4",
    "@sentry/node": "^6.2.4",
    "@sentry/tracing": "^6.2.4",
    "axios": "^0.21.1",
    "chart.js": "^3.2.0",
    "classnames": "^2.3.1",
    "cron": "^1.8.2",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
