    "redis": "^4.0.4",
    "resize-observer-polyfill": "^1.5.1",
    "sequelize": "^6.6.4",
    "stream-browserify": "^3.0.0",
    "supertest": "^6.1.3",
    "umzug": "^2.3.0",
    "unfuck-spa-shibboleth-session": "^3.1.3",
    "unfuck-utf8-headers-middleware": "^1.0.1",
